<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<table class="table ">
	  <thead class="thead-dark">
	<tr><TH>Apellidos</TH><TH>Nombres</TH>
		<tr><TH>Fecha Nac</TH><TH>Correo</TH><TH>Usuario</TH>
		<TH colspan="2"><%= link_to 'Nuevo', users_new_path %></TH></tr>
</thead>
	<tbody>
	<% @users.each do |f| %>
	
	<tr><TD><%= f.apellidos %></TD>
		<TD><%= f.nombres %></TD>
		<TD><%= f.fenac %></TD>
		<TD><%= f.email %></TD>
		<TD><%= f.username %></TD>
		<TD><%= link_to 'Editar', users_edit_path(f['id']) %></TD><TD>
<%= link_to 'Borrar', users_delete_path(f['id']) %>
		</TD>
	</tr>
    <% end %>
    </tbody>
</table>
